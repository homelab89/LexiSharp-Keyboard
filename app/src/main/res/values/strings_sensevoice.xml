    <!-- SenseVoice - Use ITN -->
    <string name="feature_sv_use_itn_off_desc">Outputs spoken-form text, such as \"one thousand two hundred thirty-four\".</string>
    <string name="feature_sv_use_itn_on_desc">Uses Inverse Text Normalization to automatically convert to written form, such as \"1234\".</string>

    <!-- SenseVoice - Preload model -->
    <string name="feature_sv_preload_off_desc">Loads the model only when needed, which may cause delay on first use.</string>
    <string name="feature_sv_preload_on_desc">Preloads the model when the keyboard appears to reduce first-use latency.</string>

    <!-- SenseVoice - Pseudo streaming -->
    <string name="label_sv_pseudo_stream">Enable pseudo streaming preview</string>
    <string name="feature_sv_pseudo_stream_off_desc">Disable pseudo streaming. Audio is recognized once at the end, same as traditional offline mode.</string>
    <string name="feature_sv_pseudo_stream_on_desc">Use VAD to split speech by pauses, sending small segments for quick local preview, then run a full-segment recognition at the end to overwrite the preview text.</string>
